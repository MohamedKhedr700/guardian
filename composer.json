{
    "name": "raid/guardian",
    "description": "Raid Authentication Package",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "raid",
        "auth",
        "authentication",
        "guardian",
        "sanctum"
    ],
    "authors": [
        {
            "name": "mohamed_khedr",
            "email": "mohamedkhedr700@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/sanctum": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Raid\\Core\\Guardian\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raid\\Core\\Guardian\\Providers\\GuardianServiceProvider"
            ]
        }
    },
    "aliases": {},
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
